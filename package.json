{
  "name": "mock-ui",
  "version": "1.1.5",
  "description": "Quickly mock REST APIs locally",
  "main": "index.js",
  "preferGlobal": true,
  "bin": "./index.js",
  "scripts": {
    "dev": "nodemon index.js --ignore database/",
    "start": "node ."
  },
  "nodemonConfig": {
    "ignore": ["database/*", "db.json"]
  },
  "keywords": [
    "api",
    "mock",
    "rest",
    "test",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliot40404/mock-server.git"
  },
  "author": "Elliot40404",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "nanoid": "^3.1.23",
    "open": "^8.2.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.9"
  }
}